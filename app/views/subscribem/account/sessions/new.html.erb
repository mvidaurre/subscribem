<h2>Sign in</h2>
<%= form_for @user, :url => sessions_url do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit "Sign in" %>
  </p>
<% end %>
<%= link_to "New User?", sign_up_url %>